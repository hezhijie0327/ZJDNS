{
    "log": {
        "access": "none",
        "loglevel": "info"
    },
    "dns": {
        "disableCache": true,
        "queryStrategy": "UseIP",
        "servers": [
            { "address": "127.0.0.1", "port": 53 }
        ]
    },
    "routing": {
        "domainMatcher": "mph",
        "domainStrategy": "IPOnDemand",
        "rules": [
            {
                "ip": [
                    "0.0.0.0/8",
                    "10.0.0.0/8",
                    "100.64.0.0/10",
                    "127.0.0.0/8",
                    "169.254.0.0/16",
                    "172.16.0.0/12",
                    "192.0.0.0/24",
                    "192.0.2.0/24",
                    "192.31.196.0/24",
                    "192.52.193.0/24",
                    "192.88.99.0/24",
                    "192.168.0.0/16",
                    "192.175.48.0/24",
                    "198.18.0.0/15",
                    "198.51.100.0/24",
                    "203.0.113.0/24",
                    "240.0.0.0/4",
                    "::/127",
                    "64:ff9b::/96",
                    "64:ff9b:1::/48",
                    "100::/64",
                    "2001::/23",
                    "2001:db8::/32",
                    "2002::/16",
                    "2620:4f:8000::/48",
                    "fc00::/7",
                    "fe80::/10",
                    "ext:/etc/xray/data/geoip.dat:cn"
                ],
                "outboundTag": "direct",
                "type": "field"
            }
        ]
    },
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 7890,
            "protocol": "socks",
            "settings": {
                "udp": true
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "vless",
            "settings": {
                "vnext": [
                    { "address": "demo.zhijie.online", "port": 443, "users": [ { "encryption": "none", "id": "99235a6e-05d4-2afe-2990-5bc5cf1f5c52" } ] }
                ]
            },
            "streamSettings": {
                "network": "xhttp",
                "security": "tls",
                "tlsSettings": {
                    "alpn": [ "h2" ],
                    "fingerprint": "randomized",
                    "minVersion": "1.3",
                    "serverName": "demo.zhijie.online"
                },
                "xhttpSettings": {
                    "host": "demo.zhijie.online",
                    "mode": "packet-up",
                    "noGRPCHeader": false,
                    "noSSEHeader": false,
                    "path": "/XHTTP4VLESS",
                    "scMaxBufferedPosts": 32,
                    "scMaxEachPostBytes": "131072-524288",
                    "scMinPostsIntervalMs": "50-100",
                    "xPaddingBytes": "128-1024",
                    "xmux": {
                        "cMaxLifetimeMs": "30000-120000",
                        "cMaxReuseTimes": "64-128",
                        "hKeepAlivePeriod": 5,
                        "hMaxRequestTimes": "800-900",
                        "maxConcurrency": "16-32",
                        "maxConnections": "4-8"
                    }
                }
            }
        },
        { "protocol": "freedom", "tag": "direct" }
    ]
}
